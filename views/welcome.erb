<div class="container">
  <div class="jumbotron">
    <h1>Welcome <%= name %> to Midas Project</h1> 
  </div>
</div>
<div class="container">
  <h2>Frontend Progress</h2>
  <div class="progress">
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="<%= prog_frontend%>" aria-valuemin="0" aria-valuemax="100" style="width:<%= prog_frontend%>%">
     
    </div>
  </div>

  <h2>Backend Progress</h2>
  <div class="progress">
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="<%= prog_backend%>" aria-valuemin="0" aria-valuemax="100" style="width:<%= prog_backend%>%">
     
    </div>
  </div>
</div>

